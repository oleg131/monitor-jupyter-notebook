{
  "name": "Monitor jupyter notebook",
  "version": "0.1",
  "description": "Show icon according to jupyter notebook busy-idle status.",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "browser_action": {
      "default_title": "Monitor jupyter notebook",
      "default_icon": "transparent.png"
  },
  "background": {
    "scripts": ["background.js"]
    },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
